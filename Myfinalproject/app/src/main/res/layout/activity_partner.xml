<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#457B9D"
    tools:context=".partnerActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="325dp"
        android:layout_height="126dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="42dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="43dp"
        app:srcCompat="@drawable/logo_no_backround" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="180dp"
        android:layout_height="47dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="37dp"
        android:background="@drawable/editext_custom"
        android:drawableStart="@drawable/name_icon"
        android:drawablePadding="8dp"
        android:ems="10"
        android:hint="owner name"
        android:inputType="textPersonName"
        android:paddingStart="15dp"
        android:paddingEnd="10dp"
        android:textColor="@color/black"
        android:textColorHint="@android:color/darker_gray"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextcompanyName"
        android:layout_width="180dp"
        android:layout_height="47dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="213dp"
        android:layout_marginTop="38dp"
        android:background="@drawable/editext_custom"
        android:drawableStart="@drawable/icon_bisnis"
        android:drawablePadding="2dp"
        android:ems="10"
        android:hint="Business name"
        android:paddingEnd="10dp"
        android:inputType="textPersonName"
        android:paddingStart="10dp"
        android:textColor="@color/black"
        android:textColorHint="@android:color/darker_gray"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextEmailAddresspatr"
        android:layout_width="339dp"
        android:layout_height="48dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="97dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/editext_custom"
        android:drawableStart="@drawable/email_icon"
        android:drawablePadding="8dp"
        android:paddingEnd="18dp"
        android:ems="10"
        android:hint="Email Address"
        android:inputType="textEmailAddress"
        android:paddingStart="15dp"
        android:textColor="@color/black"
        android:textColorHint="@android:color/darker_gray"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextphoneAddresspart"
        android:layout_width="339dp"
        android:layout_height="48dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="157dp"
        android:paddingEnd="18dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/editext_custom"
        android:drawableStart="@drawable/icon_phonen"
        android:drawablePadding="8dp"
        android:ems="10"
        android:hint="Phone Number"
        android:inputType="phone"
        android:paddingStart="15dp"
        android:textColor="@color/black"
        android:textColorHint="@android:color/darker_gray"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextAddressabout"
        android:layout_width="wrap_content"
        android:layout_height="203dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="37dp"
        android:layout_marginTop="226dp"
        android:layout_marginEnd="35dp"
        android:background="@drawable/editext_custom"
        android:drawablePadding="8dp"
        android:ems="10"
        android:gravity="top"
        android:paddingEnd="18dp"
        android:hint="About Your Business : .... "
        android:inputType="textPersonName"
        android:paddingStart="20dp"
        android:paddingTop="20dp"
        android:textColor="@color/black"
        android:textColorHint="@android:color/darker_gray"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/buttonsendit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editTextTextAddressabout"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="110dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="109dp"
        android:layout_marginBottom="-75dp"
        android:background="@drawable/button_sign"
        android:enabled="true"
        android:text="Send"
        android:textSize="20dp" />

</RelativeLayout>